{
  "build": {
    "content": [
      {
        "files": [
          "docs/**.md",
          "docs/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/media/**",
          "images/**",
          "**/img/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [ "global.json" ],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "templates/pz"
    ],
    "postProcessors": [],
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}